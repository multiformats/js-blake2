{
  "name": "@multiformats/blake2",
  "version": "0.0.0-dev",
  "description": "Multiformats BLAKE2 implementations",
  "author": "Rod Vagg <r@va.gg>",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/multiformats/js-blake2#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/multiformats/js-blake2.git"
  },
  "bugs": {
    "url": "https://github.com/multiformats/js-blake2/issues"
  },
  "keywords": [
    "blake2",
    "hash",
    "ipfs",
    "ipld",
    "multiformats",
    "multihash"
  ],
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ],
      "src/*": [
        "*",
        "dist/*",
        "dist/src/*",
        "dist/src/*/index"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./src/index.js"
    },
    "./blake2b": {
      "types": "./dist/src/blake2b.d.ts",
      "import": "./src/blake2b.js"
    },
    "./blake2s": {
      "types": "./dist/src/blake2s.d.ts",
      "import": "./src/blake2s.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "sourceType": "module"
    }
  },
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "build": "aegir build",
    "release": "aegir release",
    "test": "aegir test",
    "test:ts": "npm run test --prefix test/ts-use",
    "test:node": "aegir test -t node --cov",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "test:electron-main": "aegir test -t electron-main",
    "dep-check": "aegir dep-check"
  },
  "dependencies": {
    "blakejs": "^1.2.1",
    "multiformats": "^12.1.0"
  },
  "devDependencies": {
    "@ipld/dag-cbor": "^9.0.0",
    "aegir": "^40.0.11"
  },
  "directories": {
    "test": "test"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  }
}
